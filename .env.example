# DLFE-LSTM-WSI项目环境变量模板
# 复制此文件为 .env 并填入实际值

# 项目配置
PROJECT_NAME=DLFE-LSTM-WSI
PROJECT_VERSION=1.0.0
ENVIRONMENT=development

# 数据路径配置
DATA_ROOT_PATH=./data
RAW_DATA_PATH=./data/raw
PROCESSED_DATA_PATH=./data/processed
RESULTS_PATH=./experiments/results

# 模型配置
MODEL_CHECKPOINT_PATH=./experiments/checkpoints
LOG_PATH=./experiments/logs

# MLflow配置（可选）
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=dlfe-lstm-wsi

# Weights & Biases配置（可选）
WANDB_PROJECT=dlfe-lstm-wsi
WANDB_ENTITY=your_username

# GPU配置
CUDA_VISIBLE_DEVICES=0